@model MyService
@{
    ViewBag.Title = Model.MetaTitle;
    ViewBag.Description = Model.MetaDiscription;
    ViewBag.Keywords = Model.MetaKeyword;
}

<div>
    <h2>
        @Model.Title
    </h2>

    <div>
        @Html.Raw(Model.MyText)
    </div>
    <div>
        <img class="img-block" src="~/images/@Model.Image" />
    </div>
</div>

